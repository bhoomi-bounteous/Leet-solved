class Solution {
    ArrayList<Integer> removeDuplicates(int[] arr) {
       ArrayList<Integer> ans=new ArrayList<>();
        int indx=1;
        int unique=arr[0];
        ans.add(arr[0]);
        for(int i=1;i<arr.length;i++){
            if(arr[i]!=unique){
                unique=arr[i];
                ans.add(arr[i]);
            }
        }
        return ans;
    }
}
