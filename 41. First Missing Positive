class Solution {
    public int firstMissingPositive(int[] nums) {
        int n=nums.length;
        int i=0;
         while(i<n){
            int indx=nums[i]-1;
            if( nums[i]>0 && nums[i]<=n && nums[indx]!=nums[i]){
               int temp=nums[indx];
               nums[indx]=nums[i];
               nums[i]=temp;
            }else{
                i++;
            }
        }
        for(int j=0;j<n;j++){
            if(j+1!=nums[j]) return j+1;
        }
        return n+1;
    }
}
