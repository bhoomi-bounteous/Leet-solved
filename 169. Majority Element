class Solution {
    public int majorityElement(int[] nums) {
        int ans=nums[0];
        int count=1;
        for(int i=1;i<nums.length;i++){
            if(ans==nums[i]) count++;
            else count--;
            if(count==0){
                ans=nums[i];
                count=1;
            }
        }
        count=0;
        for(int n : nums){
           if(n==ans) count++;
        }
        if(count>nums.length/2) return ans;
        return -1;
    }
}
