class Solution {
    public String reverseWords(String s) {
        String ns=s.trim();
        String[] arr=ns.split("\\s+");
        int n=arr.length;
        for(int i=0;i<arr.length/2;i++){
          String temp=arr[i];
          arr[i]=arr[n-1-i];
          arr[n-1-i]=temp;
        }
        StringBuilder ans=new StringBuilder();
        for(String ele : arr) ans.append(ele).append(" ");
       String ans2=ans.toString().trim();
       return ans2;
    }
}
